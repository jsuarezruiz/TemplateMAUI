            <themes:ChatBubble />
            <themes:CategoryAxis />
            <themes:CircleProgressBar />
            <themes:ComparerView />
            <themes:Divider />
            <themes:ExpanderView />
            <themes:FeedbackView />
            <themes:GridLines />
            <themes:GridSplitter />
            <themes:LineChart />
            <themes:Marquee />
            <themes:PinBox />
            <themes:PreviewSlider />
            <themes:ProgressBar />
            <themes:ProgressButton />
            <themes:PropertyGrid />
            <themes:Rate />
            <themes:SegmentedControl />
            <themes:SignatureView />
            <themes:Shield />
            <themes:Slider />
            <themes:SnackBar />
            <themes:StepBar />
            <themes:Tag />
            <themes:ToggleButton />
            <themes:ToggleSwitch />
            <themes:TreeView />
            <themes:ValueAxis />
        </ResourceDictionary.MergedDictionaries>
